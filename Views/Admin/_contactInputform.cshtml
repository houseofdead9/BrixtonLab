<div class="card card-custom example example-compact">
    <div class="card-header">
        <h3 class="card-title">Client Information</h3>
        <div class="card-toolbar">
            <div class="example-tools justify-content-center">
                <span class="example-toggle" data-toggle="tooltip" title="" data-original-title="View code"></span>
                <span class="example-copy" data-toggle="tooltip" title="" data-original-title="Copy code"></span>
            </div>
        </div>
    </div>
    <!--begin::Form-->
    <div class="card card-custom">
        <form>
            <div class="card-body">
                <div class="form-group mb-8">
                </div>
                <div class="form-group row mb-10">
                    <label class="col-2 col-form-label">Primary Contact Name<span class="text-danger">*</span></label>
                    <div class="col-4">
                        <input class="form-control" type="text" id="example-text-input" />
                    </div>
                    <label for="example-search-input" class="col-2 col-form-label">Title</label>
                    <div class="col-4">
                        <input class="form-control" type="search" id="example-search-input" />
                    </div>
                </div>
                <div class="form-group row  mb-10">
                    <label for="example-email-input" class="col-2 col-form-label">Primary Contact Phone<span class="text-danger">*</span></label>
                    <div class="col-4">
                        <input class="form-control" type="email" id="example-email-input" />
                    </div>
                    <label for="example-url-input" class="col-2 col-form-label">Primary Contact Email<span class="text-danger">*</span></label>
                    <div class="col-4">
                        <input class="form-control" type="url" id="example-url-input" />
                    </div>
                </div>
                <div class="form-group row  mb-10">
                    <label for="example-tel-input" class="col-2 col-form-label"></label>
                    <div class="col-4">
                        @*<input class="form-control" type="tel"  id="example-tel-input" />*@
                    </div>
                    <label for="example-password-input" class=" col-2 col-form-label">Location ID<span class="text-danger">*</span></label>
                    <div class="col-4">
                        <input class="form-control" type="password" id="example-password-input" />
                    </div>
                </div>

                <div class="card card-custom card-bordered mb-10">
                    <div class="card-header">
                        <div class="align-self-center fs-6 card-title">Critical Contact Information</div>
                    </div>
                    <div class="card-body">
                        <div class="card-body p-0">
                            <div class="form-group row ">
                                <div class="col-lg-4">
                                    <label>Critical Contact Name<span class="text-danger">*</span></label>
                                    <input type="email" class="form-control" />
                                </div>
                                <div class="col-lg-4">
                                    <label>Critical Contact Phone Number<span class="text-danger">*</span></label>
                                    <input type="email" class="form-control" />
                                </div>
                                <div class="col-lg-4">
                                    <label>Critical Contact Email<span class="text-danger">*</span></label>
                                    <input type="text" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group row mb-10">
                    <label class="col-2">Ordering Method:</label>
                    <div class="radio-inline col-10">
                        <label class="radio col-1">
                            <input type="radio" name="radios2" />
                            <span></span>
                            Paper
                        </label>
                        <label class="radio col-1">
                            <input type="radio" name="radios2" />
                            <span></span>
                            Electronic
                        </label>
                    </div>
                </div>
                <div class="form-group row mb-10">
                    <label class="col-2">Preffered Result Notification Method:</label>
                    <div class="checkbox-list col-lg-8">
                        <label class="checkbox checkbox-outline col-2">
                            <input type="checkbox" />
                            <span></span>
                            Web Portal
                        </label>
                        <label class="checkbox checkbox-outline col-2">
                            <input type="checkbox" />
                            <span></span>
                            HIPPAA FAX #
                        </label>
                        <label class="checkbox checkbox-outline col-2">
                            <input type="checkbox" />
                            <span></span>
                            EMR Direct
                        </label>
                        <label class="checkbox checkbox-outline col-2">
                            <input type="checkbox" />
                            <span></span>
                            EMR
                        </label>
                    </div>
                </div>

                <div class="card card-custom card-bordered mb-10">
                    <div class="card-header">
                        <div class="align-self-center fs-6 card-title">Primary Physicians Details</div>
                    </div>
                    <div class="card-body">
                        <div class="card-body p-0">
                            <div class="form-group row mb-10">
                                <label class="col-2"><b>Account Activation Type:</b></label>
                                <div class="radio-inline col-10">
                                    <label class="radio col-3">
                                        <input type="radio" name="radios2" />
                                        <span></span>
                                        Email
                                    </label>
                                    <label class="radio col-3">
                                        <input type="radio" name="radios2" />
                                        <span></span>
                                        Username
                                    </label>
                                </div>
                            </div>
                            <div class="form-group row  mb-10">
                                <label class="col-2 col-form-label">Physician Full Name</label>
                                <div class="col-lg-6">
                                    <input type="email" class="form-control" />
                                </div>
                            </div>
                            <div class="form-group row  mb-10">
                                <label class="col-2 col-form-label">NPI#</label>
                                <div class="col-lg-6">
                                    <input type="email" class="form-control" />
                                </div>
                            </div>
                            <div class="form-group row  mb-10">
                                <label class="col-2 col-form-label">State License #</label>
                                <div class="col-lg-6">
                                    <input type="email" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="card-footer">
                <div class="row">
                    <div class="col-lg-6">
                        <button type="reset" class="btn btn-primary mr-2">Save</button>
                        <button type="reset" class="btn btn-secondary">Cancel</button>
                    </div>

                </div>
            </div>
        </form>
    </div>
    <!--end::Form-->
</div>